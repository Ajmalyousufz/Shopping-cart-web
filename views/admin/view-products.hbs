<section>
    <div class="container">
        <div class="row">
            <a href="/admin/add-products" class="btn btn-success w-auto mt-4">Add Product</a>
        </div>
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.discription}}</td>
      <td><img src="{{this.image}}" alt="" style="width: 50px;"></td>
    </tr>
    {{/each}}
   
  </tbody>
</table>
    </div>
</section>